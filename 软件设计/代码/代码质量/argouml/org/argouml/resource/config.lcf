#
# add -Dlog4j.configuration=org/argouml/resource/config.lcf
# to the command line to use this log4j configuration
#
log4j.rootCategory=ERROR, CONFIG
#
log4j.category.org.argouml.application.configuration=DEBUG
# 
log4j.appender.CONFIG=org.apache.log4j.ConsoleAppender
log4j.appender.CONFIG.layout=org.apache.log4j.PatternLayout
log4j.appender.CONFIG.layout.ConversionPattern=%5p: %m (%F:%L)%n
log4j.appender.CONFIG.Threshold=DEBUG
