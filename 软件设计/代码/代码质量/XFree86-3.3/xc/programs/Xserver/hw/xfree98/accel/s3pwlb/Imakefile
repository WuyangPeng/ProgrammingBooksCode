#define IHaveSubdirs

SUBDIRS = s3newmmio s3mmio drivers

SRCS = 	s3.c s3misc.c s3cmap.c s3gc.c s3gc16.c s3gc24.c s3gc32.c s3fs.c s3ss.c \
	s3gs.c s3win.c s3init.c s3im.c s3bstor.c \
