#endif

XSecurity.o::
	RemoveFile($@)
	$(LN) $(XEXTLIBSRC)/$@ .

XAppgroup.o::
	RemoveFile($@)
	$(LN) $(XEXTLIBSRC)/$@ .

clean::
	RemoveFiles($(OBJS))

DependTarget()

