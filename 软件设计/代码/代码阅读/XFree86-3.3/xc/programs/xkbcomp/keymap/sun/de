xkb_keymap "type4_de" {
    xkb_keycodes	{ include "sun(type4_euro)"	};
    xkb_types		{ include "default"		};
    xkb_compatibility	{ include "default"		};
    xkb_symbols 	{ 
	include "sun/us(sun4)+iso9995-3(basic)+de"
    };
    xkb_geometry	{ include "sun(type4)"		};
};
