	gtimg.c fc.c bcach.c

OBJS = 	ibm8514.o cmap.o gc.o fs.o ss.o \
	gs.o win.o init.o im.o bstor.o \
	scrin.o blt.o plypt.o \
	line.o seg.o frect.o text.o \
