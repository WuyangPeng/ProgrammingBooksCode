#include <Server.tmpl>
#define IHaveSubdirs

#if XF98NECS3Server
NECS3DIR = nec
#endif
#if XF98PWSKBServer
PWSKBDIR = pwskb
#endif
#if XF98PWLBServer
PWLBDIR = pwlb
#endif
#if XF98GA968Server
GA968DIR = ga968
#endif
#if XF98GANBWAPServer || XF98NEC480Server || XF98NKVNECServer || \
    XF98WABSServer    || XF98WABEPServer  || XF98WSNAServer   || \
    XF98TGUIServer    || XF98EGCServer
GENERICDIR = generic
#endif

SUBDIRS = $(NECS3DIR) $(PWSKBDIR) $(PWLBDIR) $(GA968DIR) $(GENERICDIR)

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
